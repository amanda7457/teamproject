@model Group14_BevoBooks.Models.Shipping
@{
    ViewData["Title"] = "Edit";
}
<h2>Edit</h2>
<h4>Shipping Price</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ShippingFirst" class="control-label"></label>
                <input asp-for="ShippingFirst" class="form-control" />
                <span asp-validation-for="ShippingFirst" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ShippingAdditional" class="control-label"></label>
                <input asp-for="ShippingAdditional" class="form-control" />
                <span asp-validation-for="ShippingAdditional" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
